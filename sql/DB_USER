--drop user curanazer;
CREATE USER curanazer IDENTIFIED BY "12345Aa";
GRANT CONNECT,RESOURCE,DBA TO curanazer;
GRANT CREATE SESSION , GRANT ANY PRIVILEGE TO curanazer;
GRANT UNLIMITED TABLESPACE TO curanazer;
GRANT create table to curanazer;
GRANT
     CREATE ANY MATERIALIZED VIEW
   , ALTER ANY MATERIALIZED VIEW
   , DROP ANY MATERIALIZED VIEW
   , QUERY REWRITE
   , GLOBAL QUERY REWRITE
   TO curanazer;
GRANT
     CREATE ANY TABLE
   , ALTER ANY TABLE
   , DROP ANY TABLE
   , QUERY REWRITE
   , GLOBAL QUERY REWRITE
   TO curanazer;
GRANT
     CREATE ANY VIEW
   , DROP ANY VIEW
   , QUERY REWRITE
   , GLOBAL QUERY REWRITE
   TO curanazer;
ALTER USER curanazer DEFAULT TABLESPACE users;
COMMIT;
